
{
  "AWSEBDockerrunVersion": "2",
  "authentication": {
    "bucket": "your-s3-bucket",
    "key": "your-s3-key"
  },
  "containerDefinitions": [
    {
      "name": "client",
      "image": "usmanahmed/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "usmanahmed/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "usmanahmed/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128    
    },
    {
      "name": "nginx",
      "image": "usmanahmed/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "client",
        "server"
      ]
    }
  ]
}
